<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id = "class_no_typing_name_form" model = 'ir.ui.view'>
            <field name="name">Typing Form</field>
            <field name="model">type.create.name</field>
            <field name = "arch" type="xml">
                <form>
                    <group >
                        <field name="name"/>
                    </group>
                    <footer>
                        <button name="create_name" string="Name Create" type="object" class="btn-primary"/>
                        <button string="Cancel" special="cancel" class="btn-secondary" />
                    </footer>
                </form>
            </field>
        </record>
        <record id="typing_class_no_action" model="ir.actions.act_window">
            <field name="name">Typing Class No</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">type.create.name</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
