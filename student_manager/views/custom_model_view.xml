<odoo>
	<data>
		<record id="custom_model_list_view" model="ir.ui.view">
			<field name="name">Custom Model list</field>
			<field name="model">custom.model</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="verified" />
					<field name="data" />
					<field name="code" />
				</tree>
			</field>
		</record>

		<record id="custom_model_inherit_form_view" model="ir.ui.view">
			<field name="name">Custom Model Form</field>
			<field name="model">custom.model</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="name" />
								<field name="verified" />
							</group>
							<group>
								<field name="data" />
								<field name="code" />
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>



		<record id="custom_model_action" model="ir.actions.act_window">
			<field name="name">Custom Model</field>
			<field name="res_model">custom.model</field>
			<field name="view_mode">tree,form</field>
		</record>


	</data>
</odoo>
        